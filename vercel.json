{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60,
        "memory": 3008,
        "includeFiles": [
          "node_modules/@sparticuz/chromium-min/**"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/a/(.*)", "dest": "/api/server.js" },
    { "src": "/h/(.*)", "dest": "/api/server.js" },
    { "src": "/s/(.*)", "dest": "/api/server.js" },
    { "src": "/v/(.*)", "dest": "/api/server.js" },
    { "src": "/", "dest": "/api/server.js" }
  ]
}