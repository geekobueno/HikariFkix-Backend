{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    { "src": "/a/(.*)", "dest": "/api/server.js" },
    { "src": "/h/(.*)", "dest": "/api/server.js" },
    { "src": "/", "dest": "/api/server.js" }
  ]
}
